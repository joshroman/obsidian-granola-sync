{
  "name": "obsidian-granola-sync",
  "version": "0.0.1",
  "description": "Sync your Granola meeting notes to Obsidian",
  "main": "main.js",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/joshroman/obsidian-granola-sync.git"
  },
  "scripts": {
    "build": "esbuild src/main.ts --bundle --external:obsidian --outfile=main.js --target=es2018 --format=cjs --platform=node",
    "dev": "npm run build -- --watch",
    "test": "npm run test:unit",
    "test:all": "npm run test:unit && npm run test:e2e",
    "test:unit": "jest",
    "test:unit:watch": "jest --watch",
    "test:unit:coverage": "jest --coverage",
    "test:e2e": "wdio run wdio.conf.mts",
    "test:e2e:watch": "wdio run wdio.conf.mts --watch",
    "lint": "tsc --noEmit --skipLibCheck --project tsconfig.json",
    "typecheck": "tsc --noEmit --skipLibCheck --project tsconfig.json"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "type": "commonjs",
  "devDependencies": {
    "@testing-library/jest-dom": "^6.6.3",
    "@types/jest": "^30.0.0",
    "@types/moment": "^2.11.29",
    "@types/node": "^24.0.4",
    "@types/pako": "^2.0.3",
    "@wdio/cli": "^9.16.2",
    "@wdio/local-runner": "^9.16.2",
    "@wdio/mocha-framework": "^9.16.2",
    "@wdio/spec-reporter": "^9.16.2",
    "date-fns": "^4.1.0",
    "date-fns-tz": "^3.2.0",
    "esbuild": "^0.25.5",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "moment": "^2.30.1",
    "obsidian": "^1.8.7",
    "obsidian-launcher": "^1.3.2",
    "ts-jest": "^29.4.0",
    "ts-node": "^10.9.2",
    "tslib": "^2.8.1",
    "typescript": "^5.8.3",
    "wdio-obsidian-reporter": "^1.3.2",
    "wdio-obsidian-service": "^1.3.2"
  },
  "dependencies": {
    "@types/turndown": "^5.0.5",
    "pako": "^2.1.0",
    "turndown": "^7.2.0"
  }
}
